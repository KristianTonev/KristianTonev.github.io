{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "IMF Credit Outstanding - %GDP",
    "subtitle": "Credit outstanding for all members as % of world GDP, 1984-2022",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/KristianTonev/KristianTonev.github.io/main/IMF_improved2.csv"
  },
  "height": 300,
  "width": "container",
  "mark": {"type": "bar", "color": "#86d0bb"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "%GDP",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false,"format":"%"}
    }
  },
  "config": {}
}